{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [ 
      {
        "source": "/seefood",
        "destination": "/seefood.html"
      },
      {
        "source": "/indicrafts",
        "destination": "/indicrafts.html"
      },
      {
        "source": "/panda",
        "destination": "/panda.html"
      },
      {
        "source": "/terptunes",
        "destination": "/terptunes.html"
      }
  ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
